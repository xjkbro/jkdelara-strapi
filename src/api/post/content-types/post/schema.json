        "files",
        "videos",
        "audios"
      ]
    },
    "Published": {
      "type": "date",
      "required": true,
      "unique": false
    },
    "Content": {
      "type": "richtext"
    },
    "Resources": {
      "type": "component",
      "repeatable": true,
      "component": "socials-links.resources"
    }
  }
}
